<html>
	<head>
		<title>BulletinBoard</title>
	</head>

	<body>

		<table>
			<% for (let i = 0; i < title.length; i++) { %>
				<tr>
					<td><%= title[i].id %></td>
					<td><%= title[i].title %></td>
					<td><%= title[i].body %></td>
					<td><%= title[i].created %></td>
				</tr>
			<% } %>
		</table>

		<a href="/add">Add A Message</a>
	</body>
</html>
